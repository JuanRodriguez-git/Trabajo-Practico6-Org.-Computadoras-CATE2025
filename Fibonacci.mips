li B, 0                    #0011 0001 00000000 / 0x1100
li C, 1                    #0011 0010 00000001 / 0x3201
li IR, 2                   #0011 0100 00000010 / 0x3402
li MDR, 14                 #0011 0110 00001110 / 0x360E

sw B, I/O                  #0010 0001 1001 0000 / 0x2190
sw C, I/O                  #0010 0010 1001 0000 / 0x2290

loop:
    add	ACC, B, C          #1010 0000 0001 0010 / 0xA012
    sw ACC, I/O            #0010 0000 1001 0000 / 0x2090

    addi B, C, 0           #0100 0001 0010 0000 / 0x4120
    addi C, ACC, 0         #0100 0010 0000 0000 / 0x4200 

    li FLAGS, 1            #0011 0111 00000001 / 0x3701
    add IR, IR, FLAGS      #1010 0100 0100 0111 / 0xA447

    beq IR, MDR, end       #1011 0100 0110 0001 / 0xB461
    beq ACC, ACC, loop     #1011 0000 0000 1000 / 0xB008

end:
    syscall                #0000 000000000000 / 0x0000